@model ListObservation

    <h1>@Model.resourceType</h1>
    <table class="table">
        <thead>
            <tr>
                <th>
                    Attribute
                </th>
                <th>
                    Value
                </th>
            </tr>
            <tr>
                <td>Id</td>
                <td>@Model.id</td>
            </tr>
            <tr>
                <td>Carried out</td>
                <td>@Model.meta.lastUpdated</td>
            </tr>
            <tr>
                <td>Category</td>
                <td>@Model.category.First().coding.First().display</td>
            </tr>
            <tr>
                <td>Code</td>
                <td>@Model.code.coding.First().code </td>
            </tr>
            <tr>
                <td>Observation</td>
                <td>@Model.code.text</td>
            </tr>
            <tr>
                <td>Value</td>
                <td>@Model.valueQuantity.value &nbsp;@Model.valueQuantity.unit</td>
            </tr>
        </thead>
    </table>